{{#each surveys as |survey|}}


  <div data-id="{{survey.id}}">
    <div class="col-xs-12 col-sm-4 col-md-4 col-lg-3 spaced">
      <p> Which do you prefer? {{survey.optionOne}} or {{survey.optionTwo}}?</p>

      <a data-id= {{survey.id}} class="btn btn-primary btn-lg" data-toggle="modal" data-target="#vote" data-original-title>Vote Now!</a>
    </div>

{{/each}}
